{
    "name": "Starship Prompt",
    "id": "starship",
    "version": "1.0.4",
    "description": "Starship: The minimal, blazing-fast, and infinitely customizable prompt for any shell!",
    "options": {
        "installExa": {
            "type": "boolean",
            "default": true,
            "description": "Whether install exa."
        },
        "useNerdFontSymbols": {
            "type": "boolean",
            "default": false,
            "description": "Use nerd-font-symbols preset?"
        },
        "username": {
            "type": "string",
            "default": "automatic",
            "proposals": [
                "devcontainer",
                "vscode",
                "codespace",
                "none",
                "automatic"
            ],
            "description": "Name of a non-root user or none for root"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
